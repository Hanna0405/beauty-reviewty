rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /profiles/{uid}/{all=**} { 
      allow read: if true; 
      allow write: if request.auth != null && request.auth.uid == uid; 
    }
    match /listings/{uid}/{all=**} { 
      allow read: if true; 
      allow write: if request.auth != null && request.auth.uid == uid; 
    }
    match /{path=**} { 
      allow read, write: if false; 
    }
  }
}